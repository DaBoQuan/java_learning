<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name></display-name>
  <servlet>
    <servlet-name>SelPageServlet</servlet-name>
    <servlet-class>com.ccp.servlet.SelPageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BPageServlet</servlet-name>
    <servlet-class>com.ccp.servlet.BPageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>JqGridServlet</servlet-name>
    <servlet-class>com.ccp.servlet.JqGridServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelPageServlet</servlet-name>
    <url-pattern>/SelPageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BPageServlet</servlet-name>
    <url-pattern>/BPageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>JqGridServlet</servlet-name>
    <url-pattern>/JqGridServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>